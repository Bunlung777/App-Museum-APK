{
  "expo": {
    "name": "Museum",
    "slug": "Example-1",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-R.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-R.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bunlung.Example1"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "statusBar": {
      "backgroundColor": "#000000",
      "barStyle": "light-content",
      "hidden": false
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "19e8e9f0-832c-4f04-816d-60a0b9005432"
      }
    }
  }
}
